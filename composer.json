{
    "name": "jeancsil/flight-spy",
    "description": "Watch as much flights you want and get notified for YOUR budget. (E-mail and Slack).",
    "license": "MIT",
    "authors": [
        {
            "name": "Jean Silva",
            "email": "me@jeancsil.com"
        }
    ],
    "keywords": [
        "Skyscanner",
        "Flights",
        "Travel",
        "Trip"
    ],
    "type": "library",
    "require": {
        "php": "^5.6.6|^7.0",
        "guzzlehttp/guzzle": "^6.2",
        "php-http/guzzle6-adapter": "1.1.*",
        "symfony/dependency-injection": "^3.1",
        "symfony/class-loader": "^3.1",
        "symfony/console": "^3.1",
        "symfony/config": "3.1.*",
        "symfony/yaml": "3.1.*",
        "symfony/http-foundation": "^3.1",
        "monolog/monolog": "^1.21",
        "doctrine/annotations": "1.2.*",
        "doctrine/common": "2.7.2",
        "elasticsearch/elasticsearch": "~5.0",
        "wildbit/postmark-php": "2.2.*",
        "maknz/slack": "~1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5",
        "squizlabs/php_codesniffer": "~2.7",
        "phpmd/phpmd": "~2.4"
    },
    "autoload": {
        "psr-4": {
            "Jeancsil\\FlightSpy\\": "src"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "Jeancsil\\FlightSpy\\": "test"
      }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev"
}
